{
  "name": "ember-youtube",
  "version": "0.9.5",
  "description": "A simple Ember.js component to play and control single YouTube videos using the iframe API.",
  "keywords": [
    "ember-addon",
    "youtube",
    "video",
    "player"
  ],
  "license": "MIT",
  "repository": "https://github.com/oskarrough/ember-youtube",
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each",
    "deploy-surge": "mv dist/index.html dist/200.html; surge dist ember-youtube.surge.sh",
    "prettier": "prettier --write addon/**/* app/**/* config/**/* tests/**/*"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-concurrency": "^0.8.18"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.0.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "loader.js": "^4.2.3",
    "prettier": "1.17.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-youtube.surge.sh"
  }
}
